pipeline {
  agent any
  stages {
    stage('version') {
      steps {
        sh 'python3 --version'
      }
    }
    stage('getinfo') {
      steps {
// Run the Python script using the Anaconda Python interpreter
                sh '/Users/manasvidhankani/opt/anaconda3/bin/python getinfo.py'
      // sh 'python3 getinfo.py'
      }
    }
  }
}
