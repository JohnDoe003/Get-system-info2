pipeline {
  agent any
  stages {
    stage('version') {
      steps {
        sh 'python3 --version'
      }
    }
    stage('getinfo') {
      steps {
// Activate the Anaconda environment
sh 'source /opt/anaconda3/bin/activate && conda activate my_env'
        sh 'python3 code.py'
      }
    }
  }
}
